{
  "name": "charisma-ai",
  "version": "0.1.0",
  "private": true,
  "author": "Mohamed Abdelrazig - MAAM",
  "description": "AI-powered communication analysis platform with standardized templates - Developed by Mohamed Abdelrazig - MAAM",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "tsx scripts/run-migrations.ts && node server.js",
    "lint": "next lint",
    "setup": "node setup.js",
    "setup-admin": "tsx scripts/setup-admin.ts",
    "update-admin-password": "tsx scripts/update-admin-password.ts",
    "seed:modules": "tsx scripts/seedModules.ts",
    "init:story": "node scripts/init-story-settings.js",
    "migrate:landing": "tsx scripts/migrate-landing-features.ts",
    "verify-env": "tsx scripts/verify-env.ts",
    "verify-setup": "tsx scripts/verify-setup.ts",
    "list-users": "tsx scripts/list-users.ts",
    "quality": "tsx scripts/code-quality-check.ts",
    "quality:quick": "tsx scripts/quick-quality-check.ts",
    "quality:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix && prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test:quality": "npm run typecheck && npm run lint && npm run format:check",
    "db:reset": "tsx scripts/reset-db-only.ts",
    "db:reset-seed": "tsx scripts/reset-and-seed-db.ts",
    "db:migrate": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "migrate:prod": "tsx scripts/run-migrations.ts",
    "db:diagnose": "tsx scripts/database-diagnostics.ts",
    "db:maintenance": "tsx scripts/database-maintenance.ts",
    "update-models": "tsx scripts/update-default-models.ts",
    "perf:audit": "tsx scripts/performance-audit.ts",
    "perf:analyze": "ANALYZE=true npm run build",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "postinstall": "npx prisma generate || echo 'Prisma generate skipped in production'"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@google-cloud/aiplatform": "^4.3.0",
